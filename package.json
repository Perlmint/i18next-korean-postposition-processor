{
  "name": "i18next-korean-postposition-processor",
  "version": "0.1.1",
  "description": "korean postposition postprocessor for i18next",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p src",
    "test": "cross-env TS_NODE_PROJECT=test nyc mocha --compilers ts:ts-node/register",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "prepublish": "npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Perlmint/i18next-korean-postposition-processor.git"
  },
  "keywords": [
    "i18next",
    "korean",
    "postposition"
  ],
  "author": "Gyusun Yeom",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Perlmint/i18next-korean-postposition-processor/issues"
  },
  "homepage": "https://github.com/Perlmint/i18next-korean-postposition-processor#readme",
  "dependencies": {},
  "peerDependencies": {
    "i18next": "^8.4.0"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "README.md"
  ],
  "devDependencies": {
    "@types/chai": "^4.1.1",
    "@types/i18next": "^8.4.2",
    "@types/mocha": "^2.2.46",
    "chai": "^4.1.2",
    "cross-env": "^5.1.3",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1",
    "ts-node": "^4.1.0",
    "typescript": "^2.6.2"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "test",
      "coverage"
    ],
    "reporter": [
      "html"
    ],
    "all": true  
  }
}
